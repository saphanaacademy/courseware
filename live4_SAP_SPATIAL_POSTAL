CREATE SCHEMA DEVTEST;

DROP TABLE DEVTEST.SPATIALTEST;
CREATE COLUMN TABLE DEVTEST.SPATIALTEST (
	RN TINYINT PRIMARY KEY GENERATED ALWAYS AS IDENTITY, 
	PT ST_POINT(0)
);

INSERT INTO DEVTEST.SPATIALTEST VALUES (NEW ST_POINT(1,1));
INSERT INTO DEVTEST.SPATIALTEST VALUES (NEW ST_POINT(1,2));
INSERT INTO DEVTEST.SPATIALTEST VALUES (NEW ST_POINT(2,2));
INSERT INTO DEVTEST.SPATIALTEST VALUES (NEW ST_POINT(3,2));
--NEW ST_Polygon('Polygon((0 0, 2 0, 2 2, 0 2, 0 0))')

SELECT 
	RN
	PT
FROM DEVTEST.SPATIALTEST;

